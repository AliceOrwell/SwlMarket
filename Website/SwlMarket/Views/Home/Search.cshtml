@model IEnumerable<SwlMarket.Models.Price>

<h2>Search Results</h2>

@if ((bool)ViewData["HasMaxItems"])
{
    <span><strong>Search results limited to @ViewData["MaxItemCount"]. You may want to narrow your search.</strong></span>
}
@if (Model.Any())
{
    @Html.Partial("PriceDataTable", Model)
}
else
{
    <span>No results</span>
}